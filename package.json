{
  "name": "@acrewity/n8n-nodes-acrewity",
  "version": "0.1.12",
  "description": "n8n community node for Acrewity API - A unified API platform with 20+ utility services including PDF processing, data conversion, QR codes, and more",
  "license": "MIT",
  "homepage": "https://acrewity.com",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "acrewity",
    "api",
    "pdf",
    "conversion",
    "qr-code",
    "uuid",
    "markdown",
    "excel",
    "json"
  ],
  "author": {
    "name": "Acrewity",
    "email": "support@acrewity.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acrewity/acrewity-n8n.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/AcrewityApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Acrewity/Acrewity.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/eslint-plugin-community-nodes": "^0.7.0",
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
